  script(type='text/javascript').
    jQuery(document).ready(function() {
      //- Carusel Slider
      jQuery('#example6').showbizpro({
        dragAndScroll: "off",
        visibleElementsArray: [3, 3, 1, 1],
        carousel: "on",
        entrySizeOffset: 0,
        allEntryAtOnce: "off",
